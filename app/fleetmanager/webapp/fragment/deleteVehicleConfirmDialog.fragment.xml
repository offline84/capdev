<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
<Dialog type="Message" title="Confirm delete"> 
    <Text text=" Do you really want to delete vehicle {vehicle_model>/BRAND} {vehicle_model>/MODEL} ?"/>
    <beginButton>
    <Button type="Accept" press=".deleteVehicle" text="Yes, Delete this vehicle"/>
    </beginButton>
    <endButton>
    <Button type="Reject" press=".cancelDeleteVehicle" text="No, keep it"/>
    </endButton>
</Dialog>
</core:FragmentDefinition>